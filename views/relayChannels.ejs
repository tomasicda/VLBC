<% include partials/header.ejs %>
<% include partials/adminSideBar.ejs %>

<div id="wrapper">
    <h1 class="text-center vlbc-colour-orange">Relay Channels</h1>
    <div class="vlbc-table">
        <table class="table table-bordered table-striped table-hover">
            <thread>
                <tr>
                    <th>Channel #</th>
                    <th>Current status</th>
                    <th>Switch Counts</th>
                    <th>Reset Counts</th>
                </tr>
            </thread>
            <tbody>
            <% allChannels.forEach(function(channel) { %>
                <tr>
                    <td><%= channel.channelNumber %></td>
                    <td><%= channel.status %></td>
                    <td><%= channel.switchCount %></td>
                    <td><button class="btn btn-primary" onclick="myFunction()">Reset counts</button></td>
                </tr>
            <% }); %>
            </tbody>
        </table>
    </div>
</div>
<% include partials/footer.ejs %>